  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
